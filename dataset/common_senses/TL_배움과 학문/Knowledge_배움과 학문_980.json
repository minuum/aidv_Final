            "reference_text": null,
            "role": "전문가",
            "search_URL": null,
            "search_query": null,
            "terminate": false,
            "text": "물론. 나도 친구 집에 놀러 갔다가 본 적이 있어.",
            "turn_id": "BK22006370-1",
            "utterance_id": "BK22006370.2"
